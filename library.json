{
  "name": "GeoLinker",
  "version": "1.0.0",
  "description": "A library for sending GPS and sensor data to the GeoLinker Cloud API with WiFi connectivity and offline storage support. Supports ESP32, ESP8266, Raspberry Pi Pico W/Pico 2W, and Arduino UNO R4 WiFi.",
  "keywords": "gps, geolocation, wifi, iot, esp32, esp8266, pico, rp2040, uno r4",
  "repository": {
    "type": "git",
    "url": "https://github.com/Circuit-Digest/GeoLinker.git"
  },
  "authors": [
    {
      "name": "Jobit Joseph",
      "email": "jobitjoseph1@gmail.com",
      "url": "https://github.com/jobitjoseph,
      "maintainer": true
    }
    {
      "name": "Circuit Digest",
      "email": "contact@circuitdigest.com",
      "url": "https://www.circuitdigest.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://www.circuitdigest.cloud",
  "frameworks": "arduino",
  "platforms": [
    "espressif8266",
    "espressif32",
    "renesas",
    "rp2040"
  ],
  "dependencies": [
    {
      "name": "ArduinoJson",
      "version": ">=7.0.0"
    },
    {
      "name": "WiFiS3",
      "platforms": ["renesas"],
      "version": ">=1.0.0"
    },
    {
      "name": "WiFiEspAT",
      "platforms": ["*"],
      "version": ">=1.0.0"
    }
  ],
  "export": {
    "exclude": [
      "tests",
      "examples",
      "docs"
    ]
  }
}
